{
  "name": "electron_timer",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dist": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Laukey",
  "license": "CC0-1.0",
  "devDependencies": {
    "@okta/okta-auth-js": "^5.8.0",

    "electron": "^25.5.0",
    "electron-builder": "^23.6.0"
  },
  "build": {
    "appId": "com.electron.timer",
    "directory": {
      "output": "build"
    },
    "productName": "Lab timer",
    "target": "NSIS",
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true
    }
  }
}
